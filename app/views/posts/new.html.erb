
<div id="page_wrapper">
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1> Add New Post</h1>
  <br>
  <%= simple_form_for :post, url: posts_path do |f| %>

    <%= f.input :title %>
    <br>
    <%= f.input :body, as: :text, input_html: {rows: 10, cols: 10} %>
    <br>
    <%= f.submit 'Submit', class: 'btn btn-primary' %>

  <% end %>
</div>
</div>
